class Solution {
public:
    vector<bool> prefixesDivBy5(vector<int>& nums) {
        int n=nums.size();
        vector<bool> ans;
        long long val=nums[0];
        val?ans.push_back(false):ans.push_back(true);
        for(int i=1;i<n;i++){
            val=(val*2+nums[i])%5;
            (val%5)?ans.push_back(false):ans.push_back(true);
        }
        return ans;
    }
};
